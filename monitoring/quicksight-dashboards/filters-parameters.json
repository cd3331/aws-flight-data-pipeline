  "cascadingFilters": [
    {
      "cascadeId": "region-to-detailed-region",
      "parentFilter": "region-filter",
      "childFilters": [
        {
          "filterId": "detailed-region-filter",
          "filterName": "Detailed Regional Classification",
          "filterType": "CATEGORY_FILTER",
          "scope": {
            "appliedTo": "ALL_SHEETS"
          },
          "categoryFilter": {
            "column": {
              "dataSetIdentifier": "flight-positions-dataset",
              "columnName": "detailed_region"
            },
            "configuration": {
              "filterListConfiguration": {
                "matchOperator": "CONTAINS",
                "categoryValues": {
                  "dynamicValues": {
                    "parentFilterId": "region-filter",
                    "mappingConfiguration": {
                      "North America": ["US Central", "US Eastern"],
                      "Europe": ["Western Europe", "Eastern Europe"],
                      "Asia Pacific": ["East Asia"],
                      "South America": ["Other Region"],
                      "Other": ["Other Region"]
                    }
                  }
                },
                "selectAllOptions": "FILTER_ALL_VALUES"
              }
            }
          }
        }
      ]
    },
    {
      "cascadeId": "flight-phase-to-altitude",
      "parentFilter": "flight-phase-filter",
      "childFilters": [
        {
          "filterId": "phase-specific-altitude-filter",
          "filterName": "Phase-Specific Altitude Range",
          "filterType": "NUMERIC_RANGE_FILTER",
          "scope": {
            "appliedTo": "SELECTED_SHEETS",
            "includedSheets": [
              "flight-tracking-dashboard",
              "performance-analytics-dashboard"
            ]
          },
          "numericRangeFilter": {
            "column": {
              "dataSetIdentifier": "flight-positions-dataset",
              "columnName": "altitude_ft"
            },
            "includeMinimum": true,
            "includeMaximum": true,
            "rangeMinimum": {
              "dynamicValue": {
                "parentFilterId": "flight-phase-filter",
                "mappingConfiguration": {
                  "Ground": 0,
                  "Climbing": 0,
                  "Cruise": 30000,
                  "Descending": 0,
                  "In-Transit": 0
                }
              }
            },
            "rangeMaximum": {
              "dynamicValue": {
                "parentFilterId": "flight-phase-filter",
                "mappingConfiguration": {
                  "Ground": 1000,
                  "Climbing": 30000,
                  "Cruise": 50000,
                  "Descending": 30000,
                  "In-Transit": 30000
                }
              }
            }
          }
        }
      ]
    }
  ],
  "conditionalFilters": [
    {
      "conditionalFilterId": "quality-based-visibility",
      "condition": {
        "fieldExpression": "{data_quality} < {data_quality_threshold_param}",
        "operator": "LESS_THAN"
      },
      "actions": [
        {
          "actionType": "HIDE_VISUAL",
          "targetVisuals": [
            "flight-tracking-map",
            "altitude-distribution-histogram"
          ]
        },
        {
          "actionType": "SHOW_MESSAGE",
          "message": "‚ö†Ô∏è Data quality below threshold. Some visualizations hidden to maintain accuracy."
        }
      ]
    },
    {
      "conditionalFilterId": "peak-hours-enhancement",
      "condition": {
        "parameterExpression": "{time_of_day_param} IN ('Morning', 'Evening')"
      },
      "actions": [
        {
          "actionType": "MODIFY_VISUAL_PROPERTIES",
          "targetVisuals": [
            "hourly-flight-trends"
          ],
          "properties": {
            "refreshRate": "REAL_TIME",
            "dataPointLimit": 1000
          }
        }
      ]
    },
    {
      "conditionalFilterId": "anomaly-alert-trigger",
      "condition": {
        "fieldExpression": "countIf({anomaly_score} > 0.9) > 10",
        "operator": "GREATER_THAN"
      },
      "actions": [
        {
          "actionType": "HIGHLIGHT_VISUAL",
          "targetVisuals": [
            "anomaly-detection-scatter"
          ],
          "highlightColor": "#FF6347"
        },
        {
          "actionType": "SEND_ALERT",
          "alertConfiguration": {
            "message": "üö® High number of anomalies detected in current dataset",
            "severity": "HIGH",
            "recipients": ["flight-ops@company.com"]
          }
        }
      ]
    }
  ],
