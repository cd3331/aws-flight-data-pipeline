{
  "dashboardFilters": [
    {
      "filterId": "global-date-filter",
      "filterName": "Analysis Date Range",
      "filterType": "TIME_RANGE_FILTER",
      "scope": {
        "appliedTo": "ALL_SHEETS",
        "excludedSheets": []
      },
      "timeRangeFilter": {
        "column": {
          "dataSetIdentifier": "flight-positions-dataset",
          "columnName": "timestamp"
        },
        "includeMinimum": true,
        "includeMaximum": true,
        "rangeMinimumValue": {
          "parameter": "date_range_param"
        },
        "rangeMaximumValue": {
          "rollingDateConfiguration": {
            "dateDimension": "timestamp",
            "expression": "NOW()"
          }
        },
        "nullOption": "NON_NULLS_ONLY",
        "timeGranularity": "MINUTE"
      },
      "defaultConfiguration": {
        "relativeDateType": "LAST_N_DAYS",
        "relativeDateValue": 7
      }
    },
    {
      "filterId": "altitude-range-filter",
      "filterName": "Altitude Range Filter",
      "filterType": "NUMERIC_RANGE_FILTER",
      "scope": {
        "appliedTo": "SELECTED_SHEETS",
        "includedSheets": [
          "flight-tracking-dashboard",
          "performance-analytics-dashboard"
        ]
      },
      "numericRangeFilter": {
        "column": {
          "dataSetIdentifier": "flight-positions-dataset",
          "columnName": "altitude_ft"
        },
        "includeMinimum": true,
        "includeMaximum": true,
        "rangeMinimum": {
          "parameter": "altitude_range_param"
        },
        "rangeMaximum": {
          "constant": 50000
        },
        "nullOption": "NON_NULLS_ONLY",
        "aggregationFunction": "NO_AGGREGATION"
      }
    },
    {
      "filterId": "speed-range-filter",
